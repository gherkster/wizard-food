{
	"name": "directus-extension-duration",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-interface"
	],
	"type": "module",
	"directus:extension": {
		"type": "interface",
		"host": "^10.10.0",
		"path": "dist/index.js",
		"source": "src/index.ts"
	},
	"scripts": {
		"build": "directus-extension build",
		"build-remote": "directus-extension build && pnpm run push-remote",
		"push-remote": "scp dist/index.js oracle:~/docker-cloud/directus/extensions/directus-extension-duration/dist/index.js",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^11.0.10",
		"typescript": "^5.7.3",
		"vue": "^3.5.13"
	}
}